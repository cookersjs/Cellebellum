{
  "name": "cellebellum",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "install-all": "npm i -g coffeescript karma cypress-cli nodemon webpack@2.6.1 webpack-dev-server; npm i",
    "dev": "NODE_ENV=develop webpack-dev-server --config webpack/webpack.dev.js",
    "build-be": "npm run decaf-be",
    "common-javascript": "coffee -c -o .tmp/lib/ src/common/lib/",
    "decaf-be": "coffee -c -o .tmp/ src/service",
    "reporting-bs": "cp node_modules/bootstrap/dist/css/bootstrap.css .tmp/reporting/css/",
    "reporting-etc": "cp -r etc/reporting .tmp/",
    "searching-etc": "cp -r etc/searching .tmp/",
    "scripts-etc": "cp -r etc/scripts .tmp/",
    "reporting": "npm run reporting-etc && npm run reporting-bs",
    "searching": "npm run searching-etc",
    "scripts": "npm run scripts-etc",
    "run-build": "npm run build-be && node .tmp/main.js",
    "db:seed": "seed",
    "server:seeded": "NODE_ENV=testing nodemon -w src/service -w src/common -e litcoffee --exec \"npm run run-build\"",
    "server": "NODE_ENV=develop nodemon -w src/service -w src/common -e litcoffee --exec \"npm run run-build\"",
    "test": "NODE_ENV=testing karma start karma.conf.js",
    "playbook": "ansible-playbook playbook.yml"
  },
